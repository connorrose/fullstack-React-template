{
  "name": "fullstack-react-template",
  "version": "0.1.0",
  "description": "Fullstack starter template for vanilla React app with Node/Express server",
  "scripts": {
    "build": "webpack --mode production",
    "prestart": "npm run build",
    "start": "node server/server.js",
    "dev": "concurrently \"nodemon ./server/server/js\" \"webpack-dev-server --open --inline --colors --hot --mode development\"",
    "test": "jest --verbose"
  },
